doi = {10.1016/j.powtec.2015.05.034},
file = {:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Janda, Ooi - 2016 - DEM modeling of cone penetration and unconfined compression in cohesive solids.pdf:pdf},
