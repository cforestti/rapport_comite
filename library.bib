journal = {4th International Conference on Earthquake Engineering, Thessaloniki, Greece},
keywords = {absorbing boundary,domain reduction,free-field},
number = {1231},
pages = {1--10},
title = {{The use of absorbing boundaries in dynamic analyses of soil-structure interaction problems}},
year = {2007}
}
