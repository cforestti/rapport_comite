issn = {00083674},
journal = {Canadian Geotechnical Journal},
keywords = {Anisotropy,Elasticity,Rheology,Soil mechanics},
number = {10},
pages = {1520--1536},
title = {{Caract{\'{e}}risation et effet de l'anisotropie sur lecomportement de sols reconstitut{\'{e}}s}},
