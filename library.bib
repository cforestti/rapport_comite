@article{FIKRY2016,
abstract = {cdoum spoal cctoangdeu ijto {\`{a}}u eu naein saiu ugnm re{\^{o}}nltea ttiro{\`{e}}ns dime psoersta pnrto dparin{\'{e}}st{\'{e}} lse mpr{\'{e}}occaensiqsuuse sd, ec or{\'{e}}mamlisea tpioanr edxee mtopulte o luav craagpea ceitt{\'{e}} d paonrsta lnat ep.r {\'{e}}Lvei scioonn trd{\^{o}}ele s odne comportement vis-{\`{a}}-vis des sollicitations impos{\'{e}}es pendant sa dur{\'{e}}e de vie. Actuellement, il existe diff{\'{e}}rentes m{\'{e}}thodes, directes ou indirectes, permettant de r{\'{e}}aliser ce type de contr{\^{o}}le. Toutefois, l'utilisation pour cela des p{\'{e}}n{\'{e}}trom{\`{e}}tres dynamiques dans le monde a augment{\'{e}} ces 20 derni{\`{e}}res ann{\'{e}}es. En France, par exemple, d{\`{e}}s les ann{\'{e}}es 90 cette technique de contr{\^{o}}le est devenue une pratique habituelle et normalis{\'{e}}e. Parmi ces outils, le p{\'{e}}n{\'{e}}trom{\`{e}}tre PANDA{\textregistered} (Gourves, 1991) est le plus largement utilis{\'{e}}. Le principe fondamental du PANDA{\textregistered} consiste {\`{a}} enfoncer par battage manuel dans le sol un train de tiges muni d'une pointe conique {\`{a}} son extr{\'{e}}mit{\'{e}} inf{\'{e}}rieure. Pour chaque impact, la p{\'{e}}n{\'{e}}tration (e) et la r{\'{e}}sistance de pointe (qd) sont enregistr{\'{e}}es et report{\'{e}}es en fonction de la profondeur (z). La courbe qd(z) obtenue est dite p{\'{e}}n{\'{e}}trogramme. Le contr{\^{o}}le du compactage au moyen du PANDA{\textregistered} s'appuie sur la comparaison du p{\'{e}}n{\'{e}}trogramme qd(z) obtenu in situ avec l'ensemble des courbes de r{\'{e}}f{\'{e}}rence relatives au sol contr{\^{o}}l{\'{e}}. Ces derni{\`{e}}res sont regroup{\'{e}}es dans un catalogue de r{\'{e}}f{\'{e}}rence disponible pour diff{\'{e}}rents sols, leurs {\'{e}}tats hydriques et les objectifs de compactage. Ce catalogue a {\'{e}}t{\'{e}} {\'{e}}labor{\'{e}} au laboratoire {\`{a}} partir de la calibration du PANDA{\textregistered} pour une trentaine de sols GTR diff{\'{e}}rents, dans des {\'{e}}tats de compacit{\'{e}} et d'humidit{\'{e}} variables (Chaigneau, 2001). En effet, (Chaigneau, 2001) a d{\'{e}}montr{\'{e}}, en s'appuyant sur l'approche micro-m{\'{e}}canique type "Biarez-Favre", que pour un sol et un {\'{e}}tat hydrique donn{\'{e}}, il existe une relation biunivoque entre la densit{\'{e}} s{\`{e}}che (d) et la r{\'{e}}sistance de pointe (qd). Cette relation est repr{\'{e}}sent{\'{e}}e au moyen d'une courbe d{\'{e}}finie par l'expression logarithmique (d) = A * (1/w) +B*ln (qd) +C. Les valeurs de A, B et C repr{\'{e}}sentent les constantes de r{\'{e}}gression de cette relation. En suivant les m{\^{e}}mes hypoth{\`{e}}ses que (Chaigneau, 2001) et en s'appuyant sur les travaux de Biarez et Favre, on admet que les param{\`{e}}tres A, B et C sont fonction des propri{\'{e}}t{\'{e}}s physiques, d'{\'{e}}tat et d'interaction des sols. L'objectif de ce travail est de d{\'{e}}velopper un mod{\`{e}}le math{\'{e}}matique permettant de trouver les relations qui lient les constantes de r{\'{e}}gression A, B et C aux caract{\'{e}}ristiques granulom{\'{e}}triques, d'{\'{e}}tat, hydrique et plastique du sol. Pour ce faire, cette {\'{e}}tude s'appuie sur une base de donn{\'{e}}es (BDD) compos{\'{e}}e de divers types de sols, pour lesquels on dispose des courbes de calibration pour diff{\'{e}}rents degr{\'{e}}s de compactage et d'humidit{\'{e}}. Ce travail a {\'{e}}t{\'{e}} r{\'{e}}alis{\'{e}} en 4 phases, int{\'{e}}grant quelques concepts statistiques, math{\'{e}}matiques et des notions de base de la m{\'{e}}canique de sols. Dans un premier temps un {\'{e}}tat de l'art est pr{\'{e}}sent{\'{e}} concernant les sols et leurs propri{\'{e}}t{\'{e}}s physiques, d'{\'{e}}tat et d'interaction. En deuxi{\`{e}}me temps, la BDD, outil principal de ce travail, est pass{\'{e}}e en revue et compl{\'{e}}t{\'{e}}e. Par la suite on {\'{e}}tudie par le biais de l'{\'{e}}tablissement de matrices de corr{\'{e}}lations les relations pouvant exister entre les valeurs A, B et C et les param{\`{e}}tres caract{\'{e}}ristiques des sols de la BDD. La validation des mod{\`{e}}les propos{\'{e}}s est r{\'{e}}alis{\'{e}}e exp{\'{e}}rimentalement pour de sols appartenant {\`{a}} la BDD, mais aussi pour de nouveaux sols. Une comparaison des courbes de calibration mesur{\'{e}}es au laboratoire et celles estim{\'{e}}es {\`{a}} partir des mod{\`{e}}les pr{\'{e}}sent{\'{e}}s est ainsi effectu{\'{e}}e. Les r{\'{e}}sultats de ce travail permettront de d{\'{e}}terminer, {\`{a}} partir de param{\`{e}}tres facilement identifiables au laboratoire (granulom{\'{e}}trie, plasticit{\'{e}}, teneur eau {\ldots}), les constantes de r{\'{e}}gression A, B et C pour tout sol non inclus dans la BDD originellement d{\'{e}}velopp{\'{e}}e par (Chaigneau, 2001). Il s'agit alors d'une avanc{\'{e}}e importante dans le d{\'{e}}veloppement et l'unification du contr{\^{o}}le du compactage au moyen du p{\'{e}}n{\'{e}}trom{\`{e}}tre dynamique Panda{\textregistered}},
author = {FIKRY, Marwane},
file = {:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/FIKRY - 2016 - Elaboration d'un mod{\`{e}}le math{\'{e}}matique pour le contr{\^{o}}le de compactage au moyen du p{\'{e}}n{\'{e}}trom{\`{e}}tre dynamique PANDA.pdf:pdf},
number = {0},
title = {{Elaboration d'un mod{\`{e}}le math{\'{e}}matique pour le contr{\^{o}}le de compactage au moyen du p{\'{e}}n{\'{e}}trom{\`{e}}tre dynamique PANDA}},
volume = {33},
year = {2016}
}
@article{BUNG1991,
abstract = {Dans toute {\'{e}}tude g{\'{e}}otechnique, la mod{\'{e}}lisation est une {\'{e}}tape d{\'{e}}cisive qui conditionne la qualit{\'{e}} des analyses de diagnostic ou de pr{\'{e}}vision du comportement des sols et des ouvrages. Un mod{\`{e}}le n'est pas seulement une s{\'{e}}rie d'{\'{e}}quations repr{\'{e}}sentant le comportement physique ou m{\'{e}}canique du sol, c'est aussi une repr{\'{e}}sentation g{\'{e}}om{\'{e}}trique de l'espace, qui d{\'{e}}limite les couches ou volumes occup{\'{e}}s par chaque mat{\'{e}}riau (sol, roche, eau, b{\'{e}}ton, m{\'{e}}tal, g{\'{e}}osynth{\'{e}}tiques, etc.) et pr{\'{e}}cise la place des conditions aux limites et des interfaces, avec leurs conditions de contact. Les mod{\`{e}}les utilis{\'{e}}s dans les {\'{e}}tudes de m{\'{e}}canique des sols sont tr{\`{e}}s divers. Les m{\'{e}}thodes de calcul classiques admettent en g{\'{e}}n{\'{e}}ral des g{\'{e}}om{\'{e}}tries simplifi{\'{e}}es (couche de sol homog{\`{e}}ne, massif semi-infini) et r{\'{e}}duisent souvent le comportement du sol {\`{a}} des relations unidimensionnelles (th{\'{e}}orie de la consolidation) ou bidimensionnelles planes (calculs de stabilit{\'{e}} de pente, sout{\`{e}}nements) ou axisym{\'{e}}triques (r{\'{e}}seaux de drains ou de colonnes, pieux, galeries de tunnels). Les calculs tridimensionnels sont limit{\'{e}}s aux {\'{e}}quations de l'{\'{e}}lasticit{\'{e}} lin{\'{e}}aire pour les fondations superficielles et {\`{a}} certaines {\'{e}}tudes d'{\'{e}}coulements. Les relations entre les forces ou contraintes et les d{\'{e}}placements ou d{\'{e}}formations sont souvent lin{\'{e}}aires et isotropes pour les calculs de d{\'{e}}formations, et de type rigide-plastique pour les calculs de stabilit{\'{e}}. Les progr{\`{e}}s des ordinateurs et des m{\'{e}}thodes d'analyse num{\'{e}}rique permettent de d{\'{e}}passer les limitations g{\'{e}}om{\'{e}}triques et rh{\'{e}}ologiques des m{\'{e}}thodes de calcul traditionnelles et d'aborder l'{\'{e}}tude de probl{\`{e}}mes aux g{\'{e}}om{\'{e}}tries et lois de comportement complexes, combinant les concepts classiques de compressibilit{\'{e}}, de consolidation primaire et de compression secondaire, de r{\'{e}}sistance au cisaillement, d'{\'{e}}tats limites de pouss{\'{e}}e-but{\'{e}}e ou de portance. Cette approche globale passe par la d{\'{e}}finition d'une loi de comportement sp{\'{e}}cifique {\`{a}} chaque type de sol et par l'utilisation de techniques num{\'{e}}riques appropri{\'{e}}es. Le d{\'{e}}veloppement de ces lois de comportement (ou mod{\`{e}}les rh{\'{e}}ologiques) s'appuie {\`{a}} la fois sur les sch{\'{e}}mas th{\'{e}}oriques de la m{\'{e}}canique des milieux continus ({\'{e}}lasticit{\'{e}}, plasticit{\'{e}}, viscosit{\'{e}} et leurs combinaisons) et sur les r{\'{e}}sultats d'{\'{e}}tudes exp{\'{e}}rimentales en laboratoire et en place. Suivant l'influence dominante, on peut ainsi obtenir soit des lois tr{\`{e}}s complexes, qui cherchent {\`{a}} reproduire les moindres fluctuations des courbes exp{\'{e}}rimentales, soit des mod{\`{e}}les plus simples qui se limitent {\`{a}} la repr{\'{e}}sentation des aspects essentiels du comportement des sols r{\'{e}}els.},
author = {BUNG, H. and COMBESCURE, A. and FORESTIER, A. and LEPAREUX, M. and GALON, P.},
doi = {10.1051/jp4:19913132},
file = {:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/BUNG et al. - 1991 - Lois De Comportement Et Mod{\'{e}}lisation Des Impacts.pdf:pdf},
issn = {1155-4339},
journal = {Le Journal de Physique IV},
number = {C3},
pages = {C3--937--C3--942},
title = {{Lois De Comportement Et Mod{\'{e}}lisation Des Impacts}},
volume = {01},
year = {1991}
}
@article{Dienstmann2016,
abstract = {{\textcopyright} 2016 Australian Geomechanics Society. The assessment of rate effects on piezocone test in tailings is a necessary step in the design of tailings storage facilities, since these geomaterials often exhibit coefficients of hydraulic conductivity in the range of transitional soils. The present paper describes the application of a non-linear poroelastic model conceived to capture the transient flow effects of the soil around an expanding cylinder (Dienstmann et al, 2015), to the interpretation of piezocone tests in gold tailings. Tests carried out at constant penetration rates ranging from 0.3mm/s to 57 mm/s are used in the analysis. Results are interpreted in a space that correlates dimensionless velocity V h to cone resistance Q and degree of drainage U showing that penetration is essentially drained at normalized velocities less than about 0.1 and essentially undrained at normalized velocities greater than 10.},
