journal = {EPJ Web of Conferences},
title = {{Modeling of light dynamic cone penetration test - Panda 3 {\textregistered} in granular material by using 3D Discrete element method}},
volume = {140},
year = {2017}
}
@techreport{Videla2014b,
abstract = {En reconnaissance g{\'{e}}otechnique, une des techniques le plus utilis{\'{e}}es dans le monde est tr{\`{e}}s certainement le p{\'{e}}n{\'{e}}trom{\`{e}}tre, et parmi eux, le p{\'{e}}n{\'{e}}trom{\`{e}}tre dynamique occupe une place tr{\`{e}}s importante. Cependant, les p{\'{e}}n{\'{e}}trom{\`{e}}tres dynamiques restent des appareils rustiques et d'une technologie ancienne. De m{\^{e}}me, ces appareils fournissent tr{\`{e}}s peu d'information, notamment la r{\'{e}}sistance de pointe qd ; qui n'est pas un param{\`{e}}tre intrins{\`{e}}que du sol. Par ailleurs, le ph{\'{e}}nom{\`{e}}ne du battage reste toujours expliqu{\'{e}} par la th{\'{e}}orie de chocs de Newton, alors qu'on sait qu'il n'est pas un simple probl{\`{e}}me pouvant {\^{e}}tre expliqu{\'{e}} {\`{a}} l'aide de cette th{\'{e}}orie. C'est pour r{\'{e}}pondre {\`{a}} ces besoins qu'il a {\'{e}}t{\'{e}} d{\'{e}}velopp{\'{e}} un nouveau p{\'{e}}n{\'{e}}trom{\`{e}}tre dynamique {\'{e}}quip{\'{e}} de diff{\'{e}}rents capteurs permettant, par le biais du traitement et de l'analyse des signaux enregistr{\'{e}}s lors d'un choc, d'obtenir les param{\`{e}}tres gouvernant le comportement « effort-d{\'{e}}formation » du sol pendant l'enfoncement de la pointe. Il s'agit du p{\'{e}}n{\'{e}}trom{\`{e}}tre Panda 3. Le principe de mesure est simple et consiste {\`{a}} enregistrer et {\`{a}} d{\'{e}}coupler les ondes cr{\'{e}}{\'{e}}es par l'impact du marteau sur la t{\^{e}}te de l'appareil et {\`{a}} calculer ensuite la force, l'acc{\'{e}}l{\'{e}}ration et la vitesse subies en pointe. Ceci nous permettre de tracer la courbe charge-enfoncement pour chaque coup fourni lors du battage. L'exploitation de cette courbe permet de d{\'{e}}terminer des param{\`{e}}tres de r{\'{e}}sistance (qd, qc) et de d{\'{e}}formation du sol tels que la c{\'{e}}l{\'{e}}rit{\'{e}} des ondes (Vp et Vs), le module p{\'{e}}n{\'{e}}trom{\`{e}}trique (Ep) et l'amortissement de Smith (Js). Des nombreux essais ont {\'{e}}t{\'{e}} r{\'{e}}alis{\'{e}}s {\`{a}} fin de valider la faisabilit{\'{e}} de l'appareil ainsi que des r{\'{e}}sultats obtenus au laboratoire. Toutefois {\`{a}} l'heure actuelle des essais in-situ n'ont pas {\'{e}}t{\'{e}} tr{\`{e}}s exploit{\'{e}}s. En effet, le d{\'{e}}veloppement d'un nouvel appareillage comprend non seulement le montage de l'appareil en lui-m{\^{e}}me et l'{\'{e}}tude au laboratoire, mais aussi des {\'{e}}tudes pr{\'{e}}alables de faisabilit{\'{e}}, de validation et des propositions des modes op{\'{e}}ratoires in-situ. L'objectif de ce travail c'est ainsi de valider la pertinence in-situ de cet essai et de valider la coh{\'{e}}rence des r{\'{e}}sultats obtenus par le biais d'un comparatif avec d'autres essais. Pour ce faire nous avons r{\'{e}}alis{\'{e}} des campagnes d'essai in-situ {\`{a}} l'aide du Panda 3 et des essais g{\'{e}}otechniques (CPT, DPT, PMT,{\ldots}) et g{\'{e}}ophysiques (MASW-2D,{\ldots}) sur divers chantiers et terrains. Par ailleurs, pour compl{\'{e}}ter cette {\'{e}}tude nous proposons une relation entre le terme de pointe qd et la densit{\'{e}} s{\`{e}}che, obtenue {\`{a}} partir des {\'{e}}tudes de calibration p{\'{e}}n{\'{e}}trom{\`{e}}trique r{\'{e}}alis{\'{e}}s sur la base des donn{\'{e}}es physiques des sols appartenant {\`{a}} Polytech' Clermont-Ferrand. Cela nous permettra de compl{\'{e}}ter l'information obtenue par le Panda 3. Enfin, un analyse des param{\`{e}}tres Panda 3 (r{\'{e}}sistance et d{\'{e}}formation) est {\'{e}}galement propos{\'{e}}e afin d'identifier le param{\`{e}}tre rh{\'{e}}ologique « alpha » propos{\'{e}} par Sanglerat, nous permettant de caract{\'{e}}riser la nature des sols in-situ. Mots},
