doi = {10.19199/2017.4.0557-1405.35},
file = {:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Salgado, Han, Prezzi - 2017 - Experimental Study of the Load Response of Large Diameter Closed-ended and Open-ended Pipe Piles Installed.pdf:pdf},
keywords = {group effect,non-displacement piles,scale effect,shear band},
number = {January},
title = {{Experimental Study of the Load Response of Large Diameter Closed-ended and Open-ended Pipe Piles Installed in Alluvial Soil View project Experimental Determination of Displacement and Strain Fields around Piles using Digital Image Correlation (DIC) View p}},
