issn = {0266352X},
journal = {Computers and Geotechnics},
keywords = {Mixed-face conditions,Numerical modelling,Parametric studies,Shield tunnelling,Urban tunnelling,Validation},
pages = {97--113},
publisher = {Elsevier Ltd},
title = {{Development and validation of a 3D numerical model for TBM-EPB mechanised excavations}},
