issn = {00168505},
journal = {Geotechnique},
keywords = {Design footings/foundations,In situ testing,Laboratory tests,Settlement stiffness},
number = {5},
pages = {487--508},
title = {{Non-linear soil stiffness in routine design}},
volume = {50},
year = {2000}
