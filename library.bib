@article{Wang2019,
abstract = {To model the deep penetration process of T-bar in soft clay, an adhesive contact algorithm was developed in conjunction with the Coupled Eulerian–Lagrangian approach with consideration of the effect of strain softening. Numerical results show the simulated penetration resistance agrees well with the previous centrifuge experimental data. The failure mechanisms of the clay around the T-bar can be divided into three stages, including shallow penetration stage with global failure mechanism, partially and fully back-flow stages with local failure mechanism. Fluctuations of the penetration resistance can be explained by the formation and evolution of shear bands around the T-bar. Newly formed shear bands would intersect the previously formed shear bands in the partially back-flow stage, which results in the formation of “ear-shaped” areas rotating anticlockwise around the T-bar. The evolution of shear bands would form a similar fabric structure in the fully back-flow stage.},
author = {Wang, Teng and Liu, Wenlong and Wu, Rui and Bao, Xingxian},
doi = {10.1080/1064119X.2018.1564091},
file = {:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang et al. - 2019 - Simulation of T-bar penetration in soft clay with adhesive contact.pdf:pdf},
issn = {15210618},
journal = {Marine Georesources and Geotechnology},
keywords = {CEL,Large deformation,adhesive contact,failure mechanisms,strain softening},
number = {0},
pages = {1--10},
publisher = {Taylor {\&} Francis},
title = {{Simulation of T-bar penetration in soft clay with adhesive contact}},
url = {https://doi.org/10.1080/1064119X.2018.1564091},
volume = {0},
year = {2019}
}
@article{Villavicencio2009,
abstract = {Les r{\'{e}}sidus miniers au Chili constituent le mat{\'{e}}riau st{\'{e}}rile r{\'{e}}sultant du processus d'extraction du cuivre par broyage et flottation. Ces r{\'{e}}sidus sont compos{\'{e}}s de sols fins en suspension classifi{\'{e}}s comme des limons et des sables fins homog{\`{e}}nes. La solution la plus r{\'{e}}pandue et la plus {\'{e}}conomique pour le stockage de la grande quantit{\'{e}} de r{\'{e}}sidus produite au Chili (500 000 tonnes/an) est la construction de barrages d{\'{e}}nomm{\'{e}}s « tranques de relaves » qui pr{\'{e}}sentent une pathologie importante : glissements, surverse, d{\'{e}}formations, liqu{\'{e}}faction. Les causes de cette pathologie sont une mauvaise conception et/ou une mauvaise mise en place des mat{\'{e}}riaux. L'objectif de cette th{\`{e}}se est de d{\'{e}}velopper une m{\'{e}}thodologie de compactage des mat{\'{e}}riaux constitutifs diminuant les risques d'instabilit{\'{e}} et une m{\'{e}}thodologie d'acquisition d'informations tout au long du processus permettant de mieux estimer le risque en alimentant des mod{\`{e}}les de donn{\'{e}}es {\`{a}} introduire dans les proc{\'{e}}dures de pr{\'{e}}vision de comportement et d'analyse de fiabilit{\'{e}}. Nous avons identifi{\'{e}} les variabilit{\'{e}}s de propri{\'{e}}t{\'{e}}s physiques de base communes aux barrages de ce type et estim{\'{e}} les variabilit{\'{e}}s des param{\`{e}}tres m{\'{e}}caniques associ{\'{e}}s (DR{\%}, $\phi$' et CRR). Nous avons d{\'{e}}montr{\'{e}} que les essais p{\'{e}}n{\'{e}}trom{\'{e}}triques de type Panda associ{\'{e}}s aux relations les plus pertinentes et {\`{a}} une connaissance pr{\'{e}}alable du mat{\'{e}}riau d'apport permettent de d{\'{e}}terminer la variabilit{\'{e}} des param{\`{e}}tres DR{\%}, $\phi$' et CRR n{\'{e}}cessaires au calcul fiabiliste {\`{a}} l'{\'{e}}chelle globale mais aussi de reconnaitre les structures du barrage, connaissances compl{\'{e}}mentaires n{\'{e}}cessaires pour des analyses {\`{a}} l'{\'{e}}chelle locale. Cette d{\'{e}}monstration s'est appuy{\'{e}}e sur une importante revue bibliographique, sur de nombreux r{\'{e}}sultats d'identification de propri{\'{e}}t{\'{e}} physiques et sur l'instrumentation de 3 barrages au Chili.},
