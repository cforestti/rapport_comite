file = {::},
issn = {00380806},
journal = {Soils and Foundations},
keywords = {Dynamic response,Fracture energy,Granular material,Hopkinson bar,Pseudo poisson's ratio,Soil dynamics,Stress path (IGC: D7/e8)},
number = {1},
pages = {1--10},
title = {{3D-Hopkinson bar: New experiments for dynamic testing on soils}},
volume = {39},
year = {1999}
