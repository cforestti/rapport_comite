@article{Omidvar2017,
abstract = {Low velocity projectile penetration in soils has several important military and civilian applications. Complex soilprojectile interactions occur during penetration. Numerical models are often employed to capture these interactions. However, there is little information in the literature with respect to soil-projectile kinematic interactions. In this paper, the results of low velocity projectile penetration in granular media are presented. Refractive index matched transparent soil with mechanical properties similar to angular natural sand is used as the target granular media. High-speed imaging is adapted to capture images of an embedded axisymmetric plane within the sample during projectile penetration. Digital image correlation is used to derive the temporal evolution of the displacement field during penetration. Kinematic analysis is performed to investigate the characteristics of the soil-projectile interactions. The results indicate that the soil immediately below the projectile moves predominantly downward as the projectile approaches the soil. Radially away from the projectile, there is a transition from downward movement to horizontal movement. Once the projectile passes a given depth, the soil continues to move downward, as momentum is transferred to the soil.},
author = {Omidvar, Mehdi and Iskander, Magued},
doi = {10.1680/ijpmg.14.00003},
file = {:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Omidvar, Iskander - 2017 - Modeling of projectile penetration using transparent soils.pdf:pdf},
issn = {1346-213X},
journal = {ICSMGE 2017 - 19th International Conference on Soil Mechanics and Geotechnical Engineering},
keywords = {DIC,Digital image correlation,Penetration,Projectile,Transparent soil},
pages = {959--962},
title = {{Modeling of projectile penetration using transparent soils}},
volume = {2017-Septe},
year = {2017}
}
