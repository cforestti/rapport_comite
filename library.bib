@article{Camacho-Tauta2011,
abstract = {The shear modulus and the damping ratio at small strain are routinely obtained in the laboratory or field. The standard procedure in the laboratory is the resonant-column test (ASTM D-4015). However, the method has practical limitations because of its non-automatic execution. In the last 30 years the use of bender elements has been promoted for determining the shear wave velocity. This type of test, however, is controversial, as the results still have problems of repeatability. Therefore, there is no standard to conduct these tests. Alternative methods emerge as an attractive option to take advantage of a soil specimen. The objective of the present work was to develop and implement testing and interpretation methods based on small-strain measurements using either pulse or continuous sinusoidal signals and random noise. These techniques were implemented in a modified resonant-column apparatus equipped with a set of bender elements. The implementation included the development of an automatic control system, data acquisition system and data processing software. As a result, the improved laboratory equipment was able not only to perform resonant-column and bender element tests, but also to combine these methods to exploit the advantages of each device and to obtain additional information such as frequency effects and very small-strain stiffness. The work included the use of miniature accelerometers to study of the actual behaviour of the bender elements and the performance of the time and frequency domain testing methods. Finally, the results of a testing programme on a set of soils shown the validity of the techniques developed and verify some correlations and models.},
author = {Camacho-Tauta, Javier Fernando},
doi = {10.13140/2.1.1799.8724},
file = {:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Camacho-Tauta - 2011 - Evaluation of the small-strain stiffness of soil by non-conventional dynamic testing methods.pdf:pdf;:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Camacho-Tauta - 2011 - Evaluation of the small-strain stiffness of soil by non-conventional dynamic testing methods(2).pdf:pdf},
journal = {Instituto Superior T{\'{e}}cnico},
number = {JULY 2011},
pages = {352},
title = {{Evaluation of the small-strain stiffness of soil by non-conventional dynamic testing methods}},
volume = {PhD Thesis},
year = {2011}
}
