number = {December 2016},
pages = {232--240},
publisher = {Elsevier Ltd},
title = {{Characterization of the small-strain dynamic behaviour of silty sands; contribution of silica non-plastic fines content}},
url = {http://dx.doi.org/10.1016/j.soildyn.2017.08.008},
