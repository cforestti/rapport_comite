isbn = {MN/RC 2009-12},
keywords = {Dynamic cone penetrometer,compaction,construction qualit,light weight deflectome,pavement foundations,performance related specifications,testing},
pages = {244},
title = {{Using the Dynamic Cone Penetrometer and Light Weight Deflectometer for Construction Quality Assurance}},
