@article{Arbaoui2006,
abstract = {Les p{\'{e}}n{\'{e}}trom{\`{e}}tres permettent d'obtenir un param{\`{e}}tre global, qui est la r{\'{e}}sistance de pointe; celle-ci concerne la rupture du sol et non sa d{\'{e}}formabilit{\'{e}}. L'id{\'{e}}e est d'utiliser un p{\'{e}}n{\'{e}}trom{\`{e}}tre pour mesurer in situ les propri{\'{e}}t{\'{e}}s de d{\'{e}}formabilit{\'{e}} des sols. Nous pr{\'{e}}sentons ainsi dans cet article une nouvelle m{\'{e}}thode de mesure de la d{\'{e}}formabilit{\'{e}} des sols: un essai de chargement statique {\`{a}} l'aide d'un p{\'{e}}n{\'{e}}trom{\`{e}}tre. Il s'agit d'obtenir particuli{\`{e}}rement les trois param{\`{e}}tres m{\'{e}}caniques que sont le module d'Young E, la coh{\'{e}}sion c et l'angle de frottement interne (p. Cet article d{\'{e}}crit le dispositif de l'essai, les essais effectu{\'{e}}s selon un protocole pr{\'{e}}cis et l'interpr{\'{e}}tation des courbes exp{\'{e}}rimentales obtenues, bas{\'{e}}e notamment sur un ajustement math{\'{e}}matique {\`{a}} trois coefficients pour les essais monotones croissants. L'{\'{e}}tude men{\'{e}}e sur un mat{\'{e}}riau purement frottant montre que l'essai peut permettre une {\'{e}}valuation pertinente de la d{\'{e}}formabilit{\'{e}} du sol et de sa r{\'{e}}sistance, d'une mani{\`{e}}re simple, rapide et {\'{e}}conomique. Une mod{\'{e}}lisation axisym{\'{e}}trique par {\'{e}}l{\'{e}}ments finis de l'essai est propos{\'{e}}e, en vue d'identifier une relation entre les coefficients d'ajustement de la courbe exp{\'{e}}rimentale et les param{\`{e}}tres rh{\'{e}}ologiques E, c et $\phi$. Les conditions initiales avant l'essai, qui ont un r{\^{o}}le pr{\'{e}}dominant sur la r{\'{e}}ponse du mod{\`{e}}le, sont simul{\'{e}}es en effectuant une boucle de d{\'{e}}chargement-rechargement. Cette premi{\`{e}}re mod{\'{e}}lisation simple offre des r{\'{e}}sultats prometteurs, toutefois, la prise en compte des grandes d{\'{e}}formations devrait permettre d'affiner encore le mod{\`{e}}le.},
author = {Arbaoui, Hakim and Gourv{\`{e}}s, Roland and Bressolette, Philippe and Bod{\'{e}}, Laurent},
