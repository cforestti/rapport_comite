}
@article{JAMIOLKOWSKI2018,
abstract = {Bentonite-sand mixture has been chosen as backfilling material to seal the galleries of high lev- el toxic waste disposal repositories. In India, bentonite from Rajasthan has been chosen as the possible barrier material for high level toxic waste disposal program. This paper illustrates the role of sand ratio on swelling pressure and hydraulic conductivity of the bentonite-sand mixture. Swelling pressure tests were conducted for different sand ratios such as 75, 70, 60 and 50{\%}. Similarly, compaction tests were conducted to determine the optimum moisture content and maximum dry density of bentonite-sand mixtures, for the sand ratio of 90, 80, 75, 70, 60 and 50{\%}. The hydraulic conductivity of the bentonite-sand mixtures was found out by using falling head method. The hydraulic conductivity values range from 7×10-6 to 1.6×10-10 m/s for the sand ratios of 90 and 50{\%}, respectively. In order to compare the falling head test results, hydraulic conductivity of the speci- mens was calculated indirectly from a series of consolidation tests. The hydraulic conductivity values deter- mined from both series of tests were found to be similar. The study was suggested that the most effective sand ratio of Indian barmer bentonite-sand mixtures, should not be more than 60{\%}},
author = {JAMIOLKOWSKI, M. and GERMAINE, J. T. and LANCELLOTTA, R.},
doi = {10.1007/978-3-319-73568-9_174},
file = {:C$\backslash$:/Users/Caforestti/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/JAMIOLKOWSKI, GERMAINE, LANCELLOTTA - 2018 - New developments in field and laboratory testing of soils.pdf:pdf},
pages = {536--537},
title = {{New developments in field and laboratory testing of soils}},
year = {2018}
}
@article{JeanBiarez1981,
abstract = {Predicting the binding mode of flexible polypeptides to proteins is an important task that falls outside the domain of applicability of most small molecule and protein−protein docking tools. Here, we test the small molecule flexible ligand docking program Glide on a set of 19 non-$\alpha$-helical peptides and systematically improve pose prediction accuracy by enhancing Glide sampling for flexible polypeptides. In addition, scoring of the poses was improved by post-processing with physics-based implicit solvent MM- GBSA calculations. Using the best RMSD among the top 10 scoring poses as a metric, the success rate (RMSD ≤ 2.0 {\AA} for the interface backbone atoms) increased from 21{\%} with default Glide SP settings to 58{\%} with the enhanced peptide sampling and scoring protocol in the case of redocking to the native protein structure. This approaches the accuracy of the recently developed Rosetta FlexPepDock method (63{\%} success for these 19 peptides) while being over 100 times faster. Cross-docking was performed for a subset of cases where an unbound receptor structure was available, and in that case, 40{\%} of peptides were docked successfully. We analyze the results and find that the optimized polypeptide protocol is most accurate for extended peptides of limited size and number of formal charges, defining a domain of applicability for this approach.},
