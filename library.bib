issn = {15323641},
journal = {International Journal of Geomechanics},
keywords = {Clays,Friction,Numerical models,Penetration tests},
number = {2},
pages = {170--181},
title = {{Numerical parametric study of piezocone penetration test in clays}},
volume = {3},
year = {2003}
}
