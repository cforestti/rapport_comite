journal = {Soil Dynamics and Earthquake Engineering},
number = {December},
pages = {287--299},
publisher = {Elsevier},
title = {{Characteristics of non-uniform cross-section piles in drivability}},
url = {http://dx.doi.org/10.1016/j.soildyn.2012.07.017},
volume = {43},
year = {2012}
}
