keywords = {areia,capacidade de carga {\`{a}} tra{\c{c}}{\~{a}}o,controle durante execu{\c{c}}{\~{a}}o,estaca met{\'{a}}lica helicoidal,funda{\c{c}}{\~{o}}es profundas,modelagem f{\'{i}}sica em centr{\'{i}}fuga},
